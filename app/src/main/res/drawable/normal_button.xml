<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
       <solid
          android:color="#FC952D" />
      <stroke
          android:width="1dp"
          android:color="#FC952D"  />
      <corners
          android:radius="3dp" />
      <padding
          android:left="5dp"
          android:top="5dp"
          android:right="5dp"
          android:bottom="5dp" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#A7AAAE"
          android:endColor="#A7AAAE"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="#A7AAAE" />
      <corners
          android:radius="3dp" />
      <padding
          android:left="5dp"
          android:top="5dp"
          android:right="5dp"
          android:bottom="5dp" />
    </shape>
  </item>
</selector>

