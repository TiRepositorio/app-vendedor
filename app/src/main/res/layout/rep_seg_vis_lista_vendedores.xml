<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tvd1"
            android:layout_width="70dp"
            android:text="Codigo"
            android:gravity="end"
            style="@style/textoTabla"
            />

        <TextView
            android:id="@+id/tvd2"
            android:layout_width="280dp"
            android:text="Vendedor"
            android:gravity="start"
            style="@style/textoTabla"
            />

        <TextView
            android:id="@+id/tvd3"
            android:layout_width="80dp"
            android:text="Cli. Semana"
            android:gravity="end"
            style="@style/textoTabla"
            />

        <TextView
            android:id="@+id/tvd4"
            android:layout_width="80dp"
            android:text="Tot. Visit. Validas"
            android:gravity="end"
            style="@style/textoTabla"
            />


        <TextView
            android:id="@+id/tvd5"
            android:layout_width="80dp"
            android:text="Porc. Visitas Validas"
            android:gravity="end"
            android:layout_weight="1"
            style="@style/textoTabla"
            />

        <TextView
            android:id="@+id/tvd6"
            android:layout_width="80dp"
            android:text="No Aten."
            android:gravity="end"
            style="@style/textoTabla"
            />



        <TextView
            android:id="@+id/tvd7"
            android:layout_width="80dp"
            android:text="Fuera de Ruta"
            android:gravity="end"
            style="@style/textoTabla"
            />


        <TextView
            android:id="@+id/tvd8"
            android:layout_width="80dp"
            android:text="Total en Pareja"
            android:gravity="end"
            style="@style/textoTabla"
            />








</LinearLayout>